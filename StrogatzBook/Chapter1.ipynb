    "def dxdt(x,t):\n",
    "    return np.sin(x)\n",
    "\n",
    "u = np.linspace(-10, 10,25)\n",
    "\n",
    "soln=integrate.odeint(dxdt,y0=[1],t=u)\n",
    "soln2=integrate.odeint(dxdt,y0=[-np.pi],t=u)\n",
    "plt.plot(soln)\n",
    "plt.plot(soln2)"
   ]
